#include<iostream>
#include<vector>
int main()
{
	std::vector <int>v1;
	for(int i=0;i<100;i++)
		v1.push_back(i);
	//for(auto a: v1)
		//std::cout<<a<<std::endl;
	v1.size();
	std::cout<<v1.size();
	while(v1.begin()!=v1.end())
	auto a=v1.begin();

	std::cout<<*a<<std::endl;


	return 0;
}



