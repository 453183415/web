#include <iostream>
#include<json/json.h>
#include<mysql/mysql.h>
using namespace std;

int main() {
	Json::Value root;
	Json::FastWriter fats;
	root["a"]=("afads");
	cout << fats.write(root)<<"!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
