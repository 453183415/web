#include"Log.h"
#include<sys/types.h>
#include<unistd.h>
#include<stdio.h>
#include<stdlib.h>
#include<sys/stat.h>
#include<time.h>

Log::Log(const std::string & content):content(content){

}

std::string Log::getTime(){
			  time_t now;
	          struct tm *timenow;
	           time(&now);
	          timenow=localtime(&now);
	          return asctime(timenow);
}

