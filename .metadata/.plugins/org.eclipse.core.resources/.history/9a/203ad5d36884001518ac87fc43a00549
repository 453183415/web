#include<iostream>
#include<vector>
#include<fstream>
int main()
{

	std::ifstream in("a.txt");
	std::ofstream ou("a.txt");
	std::vector <int>v1;
	for(int i=0;i<100;i++)
		v1.push_back(i);
	for(auto a: v1)
	std::cout<<a<<std::endl;
	v1.size();
	std::cout<<v1.size()<<std::endl;;
	auto a=v1.begin();
	while(a!=v1.end()){
	//auto a=v1.begin();
		a++;
	std::cout<<*a<<std::endl;

	}




	return 0;
}



