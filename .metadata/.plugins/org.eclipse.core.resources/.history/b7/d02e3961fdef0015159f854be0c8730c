#ifndef READ_QUEUE_H_
#define READ_QUEUE_H_
#include<stdlib.h>
#include<iostream>
#include<stdio.h>
#include<unistd.h>
#include<queue>
#include<pthread.h>
template <class T>
class read_queue{
private:
	pthread_mutex_t mutex;
	std::deque<T>queue;
public:
	virtual ~ read_queue();
	read_queue();
	bool push(T & item);
	bool pop(T & item);
	int get_size();
	void clear();

};




#endif /* READ_QUEUE_H_ */
